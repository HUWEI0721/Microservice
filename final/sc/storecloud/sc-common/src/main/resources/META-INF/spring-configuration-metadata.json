{
  "groups": [
    {
      "name": "sc.db"
    },
    {
      "name": "sc.mq"
    },
    {
      "name": "sc.swagger"
    },
    {
      "name": "sc.jwt",
      "type": "com.storecloud.config.SecurityConfig",
      "sourceType": "com.storecloud.config.JwtProperties"
    },
    {
      "name": "sc.auth",
      "type": "com.storecloud.config.MvcConfig",
      "sourceType": "com.storecloud.config.AuthProperties"
    }
  ],
  "properties": [
    {
      "name": "sc.mq.host",
      "type": "java.lang.String",
      "description": "rabbitmq的地址",
      "defaultValue": "47.100.81.138"
    },
    {
      "name": "sc.mq.port",
      "type": "java.lang.Integer",
      "description": "rabbitmq的端口",
      "defaultValue": "5672"
    },
    {
      "name": "sc.mq.vhost",
      "type": "java.lang.String",
      "description": "rabbitmq的virtual-host地址",
      "defaultValue": "/scxt"
    },
    {
      "name": "sc.mq.username",
      "type": "java.lang.String",
      "description": "rabbitmq的用户名",
      "defaultValue": "scxt"
    },
    {
      "name": "sc.mq.password",
      "type": "java.lang.String",
      "description": "rabbitmq的密码",
      "defaultValue": "123321"
    },
    {
      "name": "sc.mq.listener.retry.enable",
      "type": "java.lang.Boolean",
      "description": "是否开启rabbitmq的消费者重试机制",
      "defaultValue": "true"
    },
    {
      "name": "sc.mq.listener.retry.interval",
      "type": "java.time.Duration",
      "description": "消费者重试初始失败等待时长",
      "defaultValue": "1000ms"
    },
    {
      "name": "sc.mq.listener.retry.multiplier",
      "type": "java.lang.Integer",
      "description": "失败等待时长的递增倍数",
      "defaultValue": "1"
    },
    {
      "name": "sc.mq.listener.retry.max-attempts",
      "type": "java.lang.Integer",
      "description": "消费者重试最大重试次数",
      "defaultValue": "3"
    },
    {
      "name": "sc.mq.listener.retry.stateless",
      "type": "java.lang.Boolean",
      "description": "是否是无状态，默认true",
      "defaultValue": "true"
    },
    {
      "name": "sc.db.host",
      "type": "java.lang.String",
      "description": "数据库地址",
      "defaultValue": "192.168.150.101"
    },
    {
      "name": "sc.db.port",
      "type": "java.lang.Integer",
      "description": "数据库端口",
      "defaultValue": "3306"
    },
    {
      "name": "sc.db.database",
      "type": "java.lang.String",
      "description": "数据库database名",
      "defaultValue": ""
    },
    {
      "name": "sc.db.un",
      "type": "java.lang.String",
      "description": "数据库用户名",
      "defaultValue": "root"
    },
    {
      "name": "sc.db.pw",
      "type": "java.lang.String",
      "description": "数据库密码",
      "defaultValue": "123"
    },
    {
      "name": "sc.swagger.title",
      "type": "java.lang.String",
      "description": "接口文档标题"
    },
    {
      "name": "sc.swagger.description",
      "type": "java.lang.String",
      "description": "接口文档描述"
    },
    {
      "name": "sc.swagger.email",
      "type": "java.lang.String",
      "description": "接口文档联系人邮箱"
    },
    {
      "name": "sc.swagger.concat",
      "type": "java.lang.String",
      "description": "接口文档联系人"
    },
    {
      "name": "sc.swagger.package",
      "type": "java.lang.String",
      "description": "接口controller扫描包"
    },
    {
      "name": "sc.jwt.location",
      "type": "java.lang.String",
      "description": "秘钥存储地址"
    },
    {
      "name": "sc.jwt.alias",
      "type": "java.lang.String",
      "description": "秘钥别名"
    },
    {
      "name": "sc.jwt.password",
      "type": "java.lang.String",
      "description": "秘钥文件密码"
    },
    {
      "name": "sc.jwt.tokenTTL",
      "type": "java.time.Duration",
      "description": "登录有效期"
    },
    {
      "name": "sc.auth.excludePaths",
      "type": "java.util.List",
      "description": "登录放行的路径"
    },
    {
      "name": "sc.auth.includePaths",
      "type": "java.util.List",
      "description": "登录拦截的路径"
    }
  ],
  "hints": []
}